{
  "name": "brunosabotcom",
  "private": true,
  "description": "Bruno Sabot's personal website",
  "version": "0.1.0",
  "author": "Bruno Sabot <brunosabot@gmail.com>",
  "dependencies": {
    "@types/react-helmet": "~6.1.0",
    "@typescript-eslint/eslint-plugin": "~4.14.2",
    "@typescript-eslint/parser": "~4.14.2",
    "eslint-config-react-app": "~6.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "gatsby": "~2.31.1",
    "gatsby-image": "~2.10.0",
    "gatsby-plugin-client-side-redirect": "~1.1.0",
    "gatsby-plugin-feed": "^2.12.0",
    "gatsby-plugin-google-analytics": "~2.10.0",
    "gatsby-plugin-humans-txt": "~1.1.4",
    "gatsby-plugin-manifest": "~2.12.0",
    "gatsby-plugin-offline": "~3.9.0",
    "gatsby-plugin-react-helmet": "~3.9.0",
    "gatsby-plugin-robots-txt": "^1.5.5",
    "gatsby-plugin-sharp": "~2.13.4",
    "gatsby-plugin-sitemap": "~2.11.0",
    "gatsby-plugin-typescript": "~2.11.0",
    "gatsby-plugin-web-monetization": "~1.1.0",
    "gatsby-remark-embed-gist": "~1.2.1",
    "gatsby-remark-images": "~3.10.0",
    "gatsby-source-filesystem": "~2.10.0",
    "gatsby-transformer-remark": "~2.15.0",
    "gatsby-transformer-sharp": "~2.11.0",
    "react": "~17.0.1",
    "react-dom": "~17.0.1",
    "react-helmet": "~6.1.0"
  },
  "devDependencies": {
    "@types/react": "~17.0.0",
    "@types/react-dom": "~17.0.0",
    "eslint": "~7.19.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "~7.2.0",
    "eslint-plugin-flowtype": "~5.2.0",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-jsx-a11y": "~6.4.1",
    "eslint-plugin-react": "~7.22.0",
    "eslint-plugin-react-hooks": "~4.2.0",
    "husky": "~4.3.8",
    "lint-staged": "~10.5.3",
    "prettier": "~2.2.1",
    "pretty-quick": "~3.1.0",
    "stylelint": "~13.9.0",
    "stylelint-config-css-modules": "~2.2.0",
    "stylelint-config-prettier": "~8.0.2",
    "stylelint-config-recommended": "~3.0.0",
    "stylelint-order": "~4.1.0",
    "stylelint-prettier": "~1.1.2",
    "typescript": "~4.1.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "lint:format": "prettier --write \"**/*.{css,js,jsx,json,md,ts,tsx}\"",
    "lint:css": "stylelint --fix src/**/*.css",
    "lint:ts": "eslint --fix \"src/**/*.tsx\"",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "react-scripts test",
    "test:lint-staged": "cross-env CI=true react-scripts test --findRelatedTests",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brunosabot/brunosabot.com"
  },
  "bugs": {
    "url": "https://github.com/brunosabot/brunosabot.com/issues"
  },
  "lint-staged": {
    "*.css": [
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{js,jsx}": [
      "eslint --ext .js,.ts,.tsx",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
