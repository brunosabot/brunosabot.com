{
  "name": "brunosabotcom",
  "private": true,
  "description": "Bruno Sabot's personal website",
  "version": "0.1.0",
  "author": "Bruno Sabot <brunosabot@gmail.com>",
  "dependencies": {
    "@types/react-helmet": "~6.0.0",
    "@typescript-eslint/eslint-plugin": "~3.7.0",
    "@typescript-eslint/parser": "~3.7.0",
    "eslint-config-react-app": "~5.2.1",
    "gatsby": "~2.24.11",
    "gatsby-image": "~2.4.14",
    "gatsby-plugin-client-side-redirect": "~1.1.0",
    "gatsby-plugin-feed": "^2.5.11",
    "gatsby-plugin-google-analytics": "~2.3.13",
    "gatsby-plugin-humans-txt": "~1.1.4",
    "gatsby-plugin-manifest": "~2.4.21",
    "gatsby-plugin-offline": "~3.2.21",
    "gatsby-plugin-react-helmet": "~3.3.10",
    "gatsby-plugin-sharp": "~2.6.22",
    "gatsby-plugin-sitemap": "~2.4.11",
    "gatsby-plugin-tslint": "~0.0.2",
    "gatsby-plugin-typescript": "~2.4.16",
    "gatsby-plugin-web-monetization": "~1.0.1",
    "gatsby-remark-embed-gist": "~1.2.0",
    "gatsby-remark-images": "~3.3.21",
    "gatsby-source-filesystem": "~2.3.23",
    "gatsby-transformer-remark": "~2.8.27",
    "gatsby-transformer-sharp": "~2.5.11",
    "prop-types": "~15.7.2",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "react-helmet": "~6.1.0"
  },
  "devDependencies": {
    "@types/react": "~16.9.43",
    "@types/react-dom": "~16.9.8",
    "eslint": "~7.5.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-flowtype": "~5.2.0",
    "eslint-plugin-import": "~2.22.0",
    "eslint-plugin-jsx-a11y": "~6.3.1",
    "eslint-plugin-react": "~7.20.4",
    "eslint-plugin-react-hooks": "~4.0.8",
    "husky": "~4.2.5",
    "lint-staged": "~10.2.10",
    "prettier": "~2.0.5",
    "pretty-quick": "~2.0.1",
    "stylelint": "~13.6.0",
    "stylelint-config-css-modules": "~2.2.0",
    "stylelint-config-prettier": "~8.0.2",
    "stylelint-config-recommended": "~3.0.0",
    "stylelint-order": "~4.1.0",
    "stylelint-prettier": "~1.1.2",
    "tslint": "~6.1.2",
    "tslint-config-prettier": "~1.18.0",
    "tslint-loader": "~3.5.4",
    "tslint-plugin-prettier": "~2.3.0",
    "tslint-react": "~5.0.0",
    "typescript": "~3.9.7"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "lint:format": "prettier --write \"**/*.{css,js,jsx,json,md,ts,tsx}\"",
    "lint:css": "stylelint --fix src/**/*.css",
    "lint:ts": "eslint \"src/**/*.tsx\"",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "react-scripts test",
    "test:lint-staged": "cross-env CI=true react-scripts test --findRelatedTests",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brunosabot/brunosabot.com"
  },
  "bugs": {
    "url": "https://github.com/brunosabot/brunosabot.com/issues"
  },
  "lint-staged": {
    "*.css": [
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{js,jsx}": [
      "eslint --ext .js,.ts,.tsx",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
